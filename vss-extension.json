{
    "manifestVersion": 1,
    "id": "TlsTestGate",
    "name": "SSL/TLS Test Gate",
    "version": "1.0.0",
    "publisher": "JoeGatt",
    "description": "Test public or internal endpoints for non-compliant SSL/TLS settings. This extension requires Python 3.7 or 3.8, Python 3.9 is not supported at this time.",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "tags": [
        "Security",
        "Azure Pipelines",
        "TLS",
        "SSL"
      ],
    "categories": [
        "Azure Pipelines"
    ],
    "links": {
        "support": {
          "uri": "mailto:gattjoseph@hotmail.com"
        }
      },
    "icons": {
        "default": "images/extension-icon.png"        
    },
    "files": [
        {
            "path": "buildAndReleaseTask"
        },
        {
            "path": "images", "addressable": true
        }
    ],
    "demands": [],
    "repository": {
        "type": "git",
        "uri": "https://github.com/MetLife/TlsTestGate"
    },
    "content": {
        "details": {
            "path": "overview.md"
        }
    },
    "contributions": [
        {
            "id": "custom-build-release-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "buildAndReleaseTask"
            }
        }
    ]
}